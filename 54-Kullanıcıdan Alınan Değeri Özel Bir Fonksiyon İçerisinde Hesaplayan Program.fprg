    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="sayi, i, faktoriyel, j" type="Integer" array="False" size=""/>
            <declare name="toplam" type="Real" array="False" size=""/>
            <assign variable="toplam" expression="0"/>
            <assign variable="faktoriyel" expression="1"/>
            <output expression="&quot;L&#252;tfen bir say&#305; giriniz: &quot;" newline="True"/>
            <input variable="sayi"/>
