                    <assign variable="faktoriyel" expression="faktoriyel * j"/>
                </for>
                <assign variable="toplam" expression="toplam + sayi / faktoriyel"/>
            </for>
            <output expression="&quot;Girmi&#351; oldu&#287;unuz de&#287;erin belirlenen fonksiyona g&#246;re de&#287;eri: &quot; &amp; toplam" newline="True"/>
